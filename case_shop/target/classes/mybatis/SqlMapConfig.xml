<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="board.model.BoardBean" alias="BoardAlias"/>
		<typeAlias type="category.model.CategoryBean" alias="CategoryAlias"/>
		<typeAlias type="member.model.MemberBean" alias="MemberAlias"/>
		<typeAlias type="oproduct.model.OproductBean" alias="OproductAlias"/>
		<typeAlias type="orders.model.OrdersBean" alias="OrdersAlias"/>
		<typeAlias type="product.model.ProductBean" alias="ProductAlias"/>
		<typeAlias type="review.model.ReviewBean" alias="ReviewAlias"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/board.xml"/>
		<mapper resource="mybatis/category.xml"/>
		<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/oproduct.xml"/>
		<mapper resource="mybatis/orders.xml"/>
		<mapper resource="mybatis/product.xml"/>
		<mapper resource="mybatis/review.xml"/>
	</mappers>
</configuration>