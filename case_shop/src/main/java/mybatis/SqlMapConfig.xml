<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="model.board.Board" alias="BoardAlias"/>
		<typeAlias type="model.category.Category" alias="CategoryAlias"/>
		<typeAlias type="model.member.Member" alias="MemberAlias"/>
		<typeAlias type="model.oproduct.Oproduct" alias="OproductAlias"/>
		<typeAlias type="model.orders.Orders" alias="OrdersAlias"/>
		<typeAlias type="model.product.Product" alias="ProductAlias"/>
		<typeAlias type="model.review.Review" alias="ReviewAlias"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/board.xml"/>
		<mapper resource="mybatis/category.xml"/>
		<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/oproduct.xml"/>
		<mapper resource="mybatis/orders.xml"/>
